page{
    display: flex;
    flex-direction: column;
    background: #fff;
}
.my-sel{
    width: 700rpx;
    margin: 0 auto;
    padding:10px 0;
    border-bottom: 1px solid #e5e5e5;
    display: flex;
    flex-direction: row;
    vertical-align: middle;
    font-size:16px;
}
.my-title,.my-txt{
    margin:5px 0;
}
.my-txt{
    color:#747474;
}
.my-title{
    width: 440rpx;
    font-size:14px;
}
.my-bnt{
    margin-top:5px;
}
.my-pic{
    width: 85rpx;
    height:110rpx;
    margin-right:10px;
}
.bordernone{
    width: 700rpx;
    display: flex;
    margin: 0 auto;
    flex-direction: row;
    vertical-align: middle;
    border-bottom: 1px solid #e5e5e5;
    font-size: 12px;
}
.bordernone:last-child{
    border-bottom: 0;
}
.carts-item{
    border-bottom: 16px solid #e5e5e5;
}
.my-bnt{
    display: flex;
    flex-direction: row;
    vertical-align: middle;
    margin-bottom:10rpx;
}
.gay{
    display: block;
    margin-top:5px;
    color:red;
}
.int{
    width: 50rpx;
    text-align: center;
    height: 18px;
}
input{
    display:block;
    height:0;
    text-overflow:clip;
    overflow:hidden;
    white-space:nowrap;
    font-family:UICTFontTextStyleBody;
    min-height:0;

}
.bnt{
    display: block;
    width: 34rpx;
    height:34rpx;
    border: 1px solid #000;
    text-align: center;
    line-height: 34rpx;
}
.float{
    width: 750rpx;
    position: fixed;
    bottom: 0;
    border-top:2px solid #e5e5e5;
    background:#fff;
}
.my-rightbox{
    height:85rpx; 
    display: flex;
    flex-direction: row;
    vertical-align: middle;
}
.my-count{
    width: 400rpx;
    line-height: 85rpx;
    font-size:36rpx;
    margin-right:100rpx;
    padding-left:50rpx;
}
.my-bnts{
    margin:7rpx 0;
    width: 200rpx;
    height:72rpx;
    background: #fe5337;
    line-height: 72rpx;
    text-align: center;
    color:#fff;
    font-size: 28rpx;
    font-weight: bold;
}
.bai{
    width:750rpx;
    height:8rpx;
    margin-bottom:40px;
}
.remove{
    width: 60px;
    height: 100%;
    background-color: red;
    position: absolute;
    top: 0;
    right: -60px;
    display: flex;
    justify-content: center;
    align-items: center;
}
.del{
    width: 38rpx;
    height:38rpx;
}
.img-delete{
    font-size: 14px;
    color:#777777;
    display: flex;
    flex-direction: row;
    vertical-align: middle;
    position: absolute;
    right: 70rpx;
}

.car{
    position:absolute;
    color:#e7e7e7;
    top:500rpx;
    left:240rpx;
}
.goGoodsDetail{
    position:absolute;
    top:600rpx;
    left:270rpx;
}